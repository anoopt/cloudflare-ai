{
	"name": "model-scraper",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "CLOUDFLARE_ENV=development vite",
		"start": "npm run dev",
		"build": "vite build",
		"preview": "npm run build && vite preview",
		"deploy": "npm run build && npm run deploy:production",
		"deploy:production": "wrangler deploy -e production",
		"deploy:staging": "wrangler deploy -e staging",
		"format": "biome format --write",
		"test": "vitest",
		"cf-typegen": "wrangler types",
		"lint": "biome lint --error-on-warnings",
		"lint:fix": "biome lint --fix",
		"test:ci": "vitest --watch=false",
		"type-check": "tsc --noEmit"
	},
	"dependencies": {
		"agents": "^0.0.109",
		"ai": "^4.3.16",
		"hono": "^4.8.12",
		"workers-ai-provider": "0.7.2",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.3",
		"@cloudflare/vite-plugin": "^1.11.0",
		"@cloudflare/vitest-pool-workers": "^0.8.59",
		"typescript": "5.8.3",
		"vite": "^7.0.6",
		"vitest": "~3.2.4",
		"wrangler": "^4.27.0"
	}
}
